RewriteEngine On
RewriteBase /annual-project/

# Activation
RewriteRule ^activation/([a-z0-9_-]+)/?([a-z0-9_-]*)/?$    	activation.php?log=$1&cle=$2	[NC,L]

RewriteRule ^index\.php$ - [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

#	article categorie access
RewriteRule    ^([a-z0-9_-]+)/?([a-z0-9_-]*)/?$    	index.php?p=$1&article=$2	[NC,L]

#	document error
ErrorDocument 403 /annual-project/template/documents/403.html
ErrorDocument 404 /annual-project/template/documents/404.html